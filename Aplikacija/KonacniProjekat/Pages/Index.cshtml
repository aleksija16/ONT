@page
@model IndexModel
@{
    ViewData["Title"] = "ONT";
}

<div class="text-center">
    <h1 class="display-4">Organizacija Niškog Turizma</h1>
</div>

<div class="cela-pocetna-stranica">
  <div class="row">
    <div class="kartice-sa-slikama">
      <div class="card text-center" style="width: 15rem;">
        <img src="https://b4i.travel/wp-content/uploads/2019/12/1-4.jpg" class="card-img-top" alt="...">
        <div class="card-body">
          <h5 class="card-title">Ture</h5>
          <a href="/TuraSve" class="dugme">Prikaži više</a>
        </div>
      </div>

      <div class="card text-center" style="width: 15rem;">
        <img src="https://media.istockphoto.com/photos/having-a-tour-through-the-city-streets-picture-id1071461664?k=6&m=1071461664&s=612x612&w=0&h=OsKX4zPAB6SEyv9zwwcNfsHrkfdWXulWFJ0lqPmQqQM=" class="card-img-top" alt="...">
        <div class="card-body">
          <h5 class="card-title">Vodiči</h5>
          <a href="/VodicSvi" class="dugme">Prikaži više</a>
        </div>
      </div>

      <div class="card text-center" style="width: 15rem;">
        <img src="https://t3.gstatic.com/images?q=tbn:ANd9GcR8BcfJPvlwtJr83VroRXfO-ndfFMCvPflb0XTlrOsddcStGjo6xNFaH9j2_xJEQ8hIPfXqi7Yc77fTig" class="card-img-top" alt="...">
        <div class="card-body">
          <h5 class="card-title">Znamenitosti</h5>
          <a href="/ZnamenitostSve" class="dugme">Prikaži više</a>
        </div>
      </div>

      <div class="card text-center" style="width: 15rem;">
        <img src="data:image/jpeg;base64," class="card-img-top" alt="...">
        <div class="card-body">
          <h5 class="card-title">Hall of Fame</h5>
          <a href="/HallOfFame?id=1" class="dugme">Prikaži više</a>
        </div>
      </div>
    </div>
  </div>

  <div id="carouselExampleCaptions" id-hidden="true" class="carousel slide" data-ride="carousel">

    <div class="carousel-inner">
      <div class="carousel-item active">
        <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/2/24/Panorama_Nisa.JPG/1500px-Panorama_Nisa.JPG"style="height:550px" class="d-block w-100" alt="Niška tvrđava">
        <div class="carousel-caption d-none d-md-block">
          <h2>Niš</h2>
        </div> 
      </div>
      @foreach (var item in @Model.SveZnamenitosti)
      {
        <div class="carousel-item">
        <img src="images/znamenitosti/@item.Slika" class="d-block w-100" alt="@item.NazivZnamenitosti" style="height:550px">
        <div class="carousel-caption d-none d-md-block">
          <h2>@item.NazivZnamenitosti</h2>
        </div>
      </div> 
      }
    <a class="carousel-control-prev" href="#carouselExampleCaptions" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleCaptions" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>